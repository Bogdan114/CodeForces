#include <iostream>
#include <string>
using namespace std;
int main()
{
	int n,otv=0;
	string a;
	cin >> n;
	cin >> a;
	for (int i = 0; i < a.size(); i++)
	{
		if (a[i] == 'A')
		{
			otv++;
		}
	}
	if (otv == n - otv)
		cout << "Friendship";
	else
		if (otv > n - otv)
			cout << "Anton";
		else
			cout << "Danik";
}
